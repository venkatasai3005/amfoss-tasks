#include <stdio.h>

int main(void)
{
  int s3 = 0, s5 = 0;
  int i;
   long long int t;
    scanf("%lld",&t);
    while(t--){
        int n;

scanf("%d",&n);

  for (i = 0; i < n; i++) {
    if (i % 3 == 0) {
      s3 += i;
    }
    if (i % 5 == 0) {
      s5 += i;
    }

  }
  printf("%d\n", s3 + s5);
    }
  return 0;
}
